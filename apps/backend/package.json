{
  "name": "@autolanka/backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/server.ts",
    "dev:demo": "tsx watch src/server_demo.ts",
    "dev:simple": "tsx watch src/server_simple_demo.ts",
    "start": "node dist/server.js",
    "index-workflows": "tsx src/scripts/indexWorkflows.ts",
    "lint": "eslint src --ext .ts,.tsx",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "clean": "rm -rf dist",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx prisma/seed.ts",
    "db:setup": "prisma generate && prisma migrate dev && npm run db:seed"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@autolanka/common": "file:../../packages/common",
    "@autolanka/db": "file:../../packages/db",
    "@clerk/backend": "^0.25.0",
    "@prisma/client": "^5.6.0",
    "@stripe/stripe-js": "^2.1.11",
    "@xenova/transformers": "^2.17.2",
    "aws-sdk": "^2.1489.0",
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^9.2.2",
    "bull": "^4.11.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "googleapis": "^128.0.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "openai": "^4.20.1",
    "sharp": "^0.32.6",
    "stripe": "^14.7.0",
    "svix": "^1.15.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/better-sqlite3": "^7.6.8",
    "@types/bull": "^4.10.0",
    "@types/cors": "^2.8.16",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.8.0",
    "@types/nodemailer": "^6.4.14",
    "@types/uuid": "^9.0.7",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "prisma": "^5.6.0",
    "tsx": "^4.6.0",
    "typescript": "^5.2.0"
  }
}
